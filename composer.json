{
    "name": "diffalyzer/diffalyzer",
    "description": "CLI tool to analyze PHP file dependencies from git changes for optimized test/analysis runs",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "nikic/php-parser": "^5.0",
        "symfony/console": "^7.0",
        "symfony/process": "^7.0",
        "symfony/finder": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Diffalyzer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Diffalyzer\\Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/diffalyzer"
    ],
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
